{
  "name": "turing-microservice",
  "description": "Node.js microservice template",
  "version": "0.2.0",
  "private": true,
  "author": {
    "name": "Benedikt Stemmildt",
    "email": "benedikt.stemmildt@otto.de",
    "url": "https://github.com/benestem"
  },
  "main": "target/server/server.js",
  "scripts": {
    "build-client": "webpack --config resources/webpack/webpack-client.config.js --progress",
    "build-server": "webpack --config resources/webpack/webpack-server.config.js --progress",
    "build": "npm run build-client && npm run build-server",
    "postinstall": "npm run build",
    "test": "gulp test",
    "watch": "webpack --config resources/webpack/webpack-server-dev.config.js --progress",
    "start-dev": "npm run watch & nodemon --ext html,json,js,css"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "body-parser": "1.14.2",
    "compression": "1.6.0",
    "consolidate": "0.13.1",
    "cookie-parser": "1.4.1",
    "debug": "2.2.0",
    "express": "4.13.3",
    "morgan": "1.6.1",
    "swig": "1.4.2",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0"
  },
  "devDependencies": {
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-hmre": "1.0.1",
    "chai": "3.4.1",
    "core-js": "2.0.3",
    "css-loader": "0.23.1",
    "eslint-plugin-react": "3.15.0",
    "extract-text-webpack-plugin": "1.0.1",
    "gulp": "3.9.0",
    "gulp-eslint": "1.1.1",
    "gulp-istanbul": "0.10.3",
    "gulp-mocha": "2.2.0",
    "json-loader": "0.5.4",
    "karma": "0.13.19",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chai-sinon": "0.1.5",
    "karma-coverage": "0.5.3",
    "karma-fixture": "0.2.5",
    "karma-html2js-preprocessor": "0.1.0",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "0.2.3",
    "material-ui": "0.14.2",
    "mocha": "2.3.4",
    "node-sass": "3.4.2",
    "nodemon": "1.8.1",
    "phantomjs": "1.9.19",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-tap-event-plugin": "0.2.1",
    "run-sequence": "1.1.5",
    "sass-loader": "3.1.2",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0",
    "supertest": "1.1.0"
  },
  "engines": {
    "node": "5.4.1"
  },
  "homepage": "https://github.com/otto-de/turing-microservice#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:otto-de/turing-microservice.git"
  },
  "bugs": {
    "url": "https://github.com/otto-de/turing-microservice/issues"
  },
  "license": "MIT"
}
