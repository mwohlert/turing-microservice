{
  "turing": {
    "logging": {
      "namespace": "turing-example",
      "meta": {
        "type": "turing-example"
      },
      "headers": [
        "x-origin"
      ],
      "transports": [
        {
          "type": "Console",
          "opts": {
            "prettyPrint": true,
            "colorize": true
          }
        }
      ]
    },
    "server": {
      "port": 8080,
      "routes": {
        "internal": "/turing-example/internal"
      }
    }
  },
  "turing-example": {
    "server": {
      "routes": {
        "root": "/turing-example"
      }
    },
    "mongo": {
      "host": "mongodb://localhost:27017/turing"
    }
  }
}
